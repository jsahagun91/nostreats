{
  "headers": [
    {
      "source": "/.well-known/nostr.json",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!.well-known).*)",
      "destination": "/index.html"
    }
  ]
}
